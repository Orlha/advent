#include <fstream>
#include <string>
#include <iostream>
#include <vector>

enum Place {
	Empty,
	Occupied,
	Floor,
};

int main() {
	std::fstream file { "input.txt" };
	std::vector<Place> grid;
	int width = 0;
	for(std::string tmp; std::getline(file, tmp); ) {
		width = tmp.size();
		for(auto c: tmp) {
			switch(c) {
				case 'L':
					grid.push_back(Place::Empty);
					break;
				case '#': {
					grid.push_back(Place::Occupied);
					break;
				}
				case '.': {
					grid.push_back(Place::Floor);
					break;
				}
			}
		}
	}
	for(int i = 0; i < grid.size(); ++i) {
		if((i + 1) % width == 0) {
			std::cout << "\n";
		}

	}
}
